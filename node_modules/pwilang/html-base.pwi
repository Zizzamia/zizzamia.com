<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
@html xmlns="http://www.w3.org/1999/xhtml"
    @head
        @title % block pagetitle
        @/meta charset=UTF-8
        @/meta http-equiv=content-language content="% block lang [en]"

        % block includesjs
            @script type="text/javascript" src="$_js/jquery-1.4.3.min.js"
            @script type="text/javascript" src="$_js/jquery-ui.min.js"

        @script type="text/javascript"
            % block inlinejs

        % block extrajs

        % block includescss
            @link type="text/css" rel="stylesheet" href="$_css/common.css"

        @style type="text/css"
            % block extracss
        % block extrahead

    @body
        @div #__contents__
            % block content
        @div #__footer__
            % block footer

        % if toto
            prout
        % elseif tata
            prat
        % else
            prut

